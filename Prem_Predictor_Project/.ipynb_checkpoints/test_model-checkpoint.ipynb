    "headers = {\"Content-Type\": \"application/json\"}\n",
    "data = {\"home_team\": \"Luton Town\", \"away_team\": \"Manchester City\"}\n",
    "\n",
    "response = requests.post(url, json=data, headers=headers)\n",
